doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1 Cars - Custom object table
        .cars 
            a(href="/update-cobj") Add/Update this table
            table
                thead
                    tr
                        th Name
                        th Brand
                        th Model
                tbody
                    each car in data
                        tr
                            td #{car.properties.name}
                            td #{car.properties.brand}
                            td #{car.properties.model}